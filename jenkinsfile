pipeline {
    agent {label 'maven'}
    triggers { cron('45 11 * * *')
        }
    stages {
        stage ('vcs') {
            steps {
                git branch: 'realese',
                    url: 'https://github.com/anji1649github/shopizer.git'
            }
        }
            stage ('build') {
            steps {
                sh 'mvn package'  
            }
        }
            stage ('archiveArtifacts') {
                steps {
                    junit '**/surefire-reports/*.xml'
                }
            }
        
        }
    }
